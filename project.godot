config_version=5

[application]

config/name="Nothing Shady at THIS BBQ!"
config/description="For UTS Playmakers Summertime Game Jam 2024

Created by Team Stingray"
run/main_scene="res://Levels/menus/MainMenu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.270588, 0.270588, 0.270588, 1)
boot_splash/image="res://ui_sprites/SteamTingrayGradient.png"
config/icon="res://icon.png"

[autoload]

AirtableManager="*res://online/AirtableManager.gd"
CustomerPool="*res://Interactives/Customers/CustomerPool.tscn"
ShaderLoader="*res://Autoloads/shader_loader.gd"
ScoreManager="*res://Autoloads/score_manager.gd"
GameOverManager="*res://Autoloads/game_over_manager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/mode=3
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://Autoloads/": "purple",
"res://Fonts/": "green",
"res://Interactives/": "yellow",
"res://Levels/": "blue",
"res://Player/": "green",
"res://audio/": "pink",
"res://shaders/": "green"
}

